<settings>
	<setting propertyKey="@Properties.appVersion" 
	               title="@Strings.AppVersionTitle">
		<settingConfig type="alphaNumeric" readonly="true" />
	</setting>	
	
	<!-- LAYOUT -->
    <setting propertyKey="@Properties.LayoutType" 
                    title="@Strings.LayoutTypeTitle">
        <settingConfig type="list">
            <listEntry value="0">@Strings.LAYOUTMODE_GEEK</listEntry>
            <listEntry value="1">@Strings.LAYOUTMODE_VERTICAL</listEntry>
            <listEntry value="2">@Strings.LAYOUTMODE_HORIZONTAL</listEntry>
            <listEntry value="3">@Strings.LAYOUTMODE_CIRCULAR1</listEntry>
            <listEntry value="4">@Strings.LAYOUTMODE_CIRCULAR2</listEntry>
            <listEntry value="5">@Strings.FENIX6LAYOUT</listEntry>
            <listEntry value="6">@Strings.LAYOUTMODE_FR735</listEntry>
        </settingConfig>
    </setting>        
    
    
    <!-- BATTERY -->
        <setting propertyKey="@Properties.showBatteryPercentage" 
                        title="@Strings.showBatteryPercentageTitle">
        <settingConfig type="boolean" />
    </setting>    
    <setting propertyKey="@Properties.showBatteryBar" 
                    title="@Strings.showBatteryBarTitle">
        <settingConfig type="boolean" />
    </setting>
    <setting propertyKey="@Properties.RemainingBatteryEstimate"
        title="@Strings.RemainingBatteryEstimateTitle">
        <settingConfig type="boolean" />
    </setting>      
    
    <!-- TIME -->   
    <!-- <setting propertyKey="@Properties.showTime" 
                    title="@Strings.showTimeTitle">
        <settingConfig type="boolean" />
    </setting>    
    <setting propertyKey="@Properties.showDate" 
                    title="@Strings.showDateTitle">
        <settingConfig type="boolean" />
    </setting> -->    
    <setting propertyKey="@Properties.ShowSun"
        title="@Strings.ShowSunTitle">
        <settingConfig type="boolean" />
    </setting>
    
	<!-- ACTIVITY -->
    <setting propertyKey="@Properties.showDistance" 
                    title="@Strings.showDistanceTitle">
        <settingConfig type="boolean" />
    </setting>

    <!-- BINARY -->
	<setting propertyKey="@Properties.BinaryRadius" 
	                title="@Strings.BinaryRadiusTitle">
		<settingConfig type="list">
			<listEntry value="4">@Strings.RADIUS_4</listEntry>
			<listEntry value="5">@Strings.RADIUS_5</listEntry>
			<listEntry value="6">@Strings.RADIUS_6</listEntry>
			<listEntry value="7">@Strings.RADIUS_7</listEntry>
			<listEntry value="8">@Strings.RADIUS_8</listEntry>
			<listEntry value="9">@Strings.RADIUS_9</listEntry>
		</settingConfig>
	</setting>	
    <setting propertyKey="@Properties.ShowSeconds" 
                    title="@Strings.ShowSecondsTitle">
        <settingConfig type="boolean" />
    </setting>  
    
	
	<!-- COLORS -->
	<setting propertyKey="@Properties.ForegroundColor" 
                   title="@Strings.ForegroundColorTitle">
		<settingConfig type="list">
			<listEntry value="0xFFFFFF">@Strings.COLOR_WHITE</listEntry>
			<listEntry value="0xAAAAAA">@Strings.COLOR_LT_GRAY</listEntry>
			<listEntry value="0x555555">@Strings.COLOR_DK_GRAY</listEntry>
			<listEntry value="0x000000">@Strings.COLOR_BLACK</listEntry>
			<listEntry value="0xFF0000">@Strings.COLOR_RED</listEntry>
			<listEntry value="0xAA0000">@Strings.COLOR_DK_RED</listEntry>
			<listEntry value="0xFF5500">@Strings.COLOR_ORANGE</listEntry>
			<listEntry value="0xFFAA00">@Strings.COLOR_YELLOW</listEntry>
			<listEntry value="0x00FF00">@Strings.COLOR_GREEN</listEntry>
			<listEntry value="0x00AA00">@Strings.COLOR_DK_GREEN</listEntry>
			<listEntry value="0x00AAFF">@Strings.COLOR_BLUE</listEntry>
			<listEntry value="0x0000FF">@Strings.COLOR_DK_BLUE</listEntry>
			<listEntry value="0xAA00FF">@Strings.COLOR_PURPLE</listEntry>
			<listEntry value="0xFF00FF">@Strings.COLOR_PINK</listEntry>
		</settingConfig>
	</setting>
		
    <setting propertyKey="@Properties.BackgroundColor" 
                    title="@Strings.BackgroundColorTitle">
        <settingConfig type="list">
            <listEntry value="0xFFFFFF">@Strings.COLOR_WHITE</listEntry>
            <listEntry value="0xAAAAAA">@Strings.COLOR_LT_GRAY</listEntry>
            <listEntry value="0x555555">@Strings.COLOR_DK_GRAY</listEntry>
            <listEntry value="0x000000">@Strings.COLOR_BLACK</listEntry>
            <listEntry value="0xFF0000">@Strings.COLOR_RED</listEntry>
            <listEntry value="0xAA0000">@Strings.COLOR_DK_RED</listEntry>
            <listEntry value="0xFF5500">@Strings.COLOR_ORANGE</listEntry>
            <listEntry value="0xFFAA00">@Strings.COLOR_YELLOW</listEntry>
            <listEntry value="0x00FF00">@Strings.COLOR_GREEN</listEntry>
            <listEntry value="0x00AA00">@Strings.COLOR_DK_GREEN</listEntry>
            <listEntry value="0x00AAFF">@Strings.COLOR_BLUE</listEntry>
            <listEntry value="0x0000FF">@Strings.COLOR_DK_BLUE</listEntry>
            <listEntry value="0xAA00FF">@Strings.COLOR_PURPLE</listEntry>
            <listEntry value="0xFF00FF">@Strings.COLOR_PINK</listEntry>
        </settingConfig>
    </setting>
</settings>
